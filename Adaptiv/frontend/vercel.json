{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "env": {
    "REACT_APP_API_URL": "https://adaptiv-backend.onrender.com"
  },
  "rewrites": [
    { 
      "source": "/api/:path*", 
      "destination": "https://adaptiv-backend.onrender.com/api/:path*" 
    }
  ]
}
